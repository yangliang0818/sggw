<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>查询律师信息</title>
</head>
<body>
<form method="post" action="adminqrylawyerinfo!qryLawyerInfo.action" target="qryresult">
    <DIV class=underline style="margin-left: 380px;"><STRONG class=font_24>&nbsp;律师资料查询
    </STRONG><BR>
    </DIV>
    <div style="margin-bottom: 10px;">
        <input type="radio" checked="checked"> 查询条件
    </div>
    <div align="center">
    <div style="margin-bottom: 10px;">
        <div>
            <div style="float:left;margin-right: 20px;">
                律师姓名<input type="text" name="name" size=30>
            </div>
            <div style="margin-left: 20px;">地&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;区<input type="text" name="area" size="30"></div>
        </div>
    </div>
    <div style="margin-bottom: 10px;">
        <div>
            <div style="float:left;margin-right: 20px;">
                职业证号<input type="text" name="number" size="30">
            </div>
        <div style="margin-left: 20px;">&nbsp;&nbsp;&nbsp;&nbsp;手机号<input type="text" name="phone" size="30"></div>
        </div>
    </div>
    </div>
    <div style="margin-left: 350px;">
        <input type="submit" value="查询" style="width: 80px;">
        <input type="reset" value="重置" style="width: 80px;">
    </div>
</form>
<iframe  name="qryresult" width="100%" height="600px;" frameborder="0" src="adminqrylawyerinfo.action"></iframe>
</body>
</html>