<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ taglib uri="/struts-tags" prefix="s" %>
<html>
<head>
    <title></title>
    <SCRIPT src="admin/js/Dates.js"></SCRIPT>
</head>
<body>
<form action="adminsuccess!addPress.action" method="post">
    <DIV class=underline align="center"><STRONG class=font_24>&nbsp;发布新闻
    </STRONG><BR>
    </DIV>
    <BR>
    <table width="100%" align="center">
        <tr>
            <td width="8%">地区</td>
            <td>
                <input type="text" size="50" name="area"><font color="red">*例如：上海</font>
            </td>
        </tr>
        <tr>
            <td>标题</td>
            <td>
                <input type="text" size="50" name="title"><font color="red">*例如：新闻标题</font>
            </td>
        </tr>
        <tr>
            <td>内容</td>
            <td>
                <textarea rows="15" cols="80" name="content"></textarea><font color="red">*描述新闻的内容</font>
            </td>
        </tr>
        <tr>
            <td>新闻时间</td>
            <td>
                <input type="text" name="date" size="30" onclick="setDay(this)"><font color="red">*格式：2012-12-05</font><br/>
            </td>
        </tr>
        <tr align="center">
            <td>
                &nbsp;
            </td>
            <td>
                &nbsp;
            </td>
        </tr>
    </table>
    <table width="70%">
        <tr align="center">
            <td align="center">
                <input type="submit" value="提交">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                <input type="reset" value="重置">
            </td>
            <td>
                &nbsp;
            </td>
        </tr>
    </table>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
</form>
</body>
</html>