/*******
 description:首页样式;
 author:chenlin;
 date:2013-08-15;
 *********/
@import "public.css";

/************************************part01***********************************/
.part01 {
    margin: 10px 0;
    padding: 6px;
    height: 358px;
    background: #434343;
    overflow: hidden;
}

.findbox {
    border: 1px solid #303030;
    background: #616161;
    width: 484px;
    border-right: 0px;
    float: left;
    padding: 0 13px 0 8px;
    float: left;
}

.searchform {
    border-bottom: 1px solid #4b4b4b;
    height: 187px;
}

.searchform .formItem {
    height: 28px;
    padding: 23px 0 4px 0;
}

.searchform .option {
    display: inline-block;
    *display: inline;
    _zoom: 1;
}

.searchform label, .searchform em {
    font: 14px/28px "微软雅黑";
    color: #FFFFFF;
    padding: 0 2px 0 0;
    _padding: 0 1px 0 0;
    display: inline-block;
    vertical-align: middle;
}

.searchform label {
    float: left;
    padding: 0 6px 0 15px;
}

.selectBox {
    border: 1px solid #000000;
    height: 26px;
    float: left;
    width: 138px;
    background: #ffffff;
}

.select {
    border: 0 none;
    height: 23px;
    line-height: 23px;
    padding: 2px 0 3px 0;
    width: 136px;
    margin-top: 1px;
    font-size: 14px;
    font-family: "微软雅黑"
}

.myselect {
    border: 0 none;
    height: 23px;
    line-height: 23px;
    padding: 2px 0 3px 0;
    width: 100px;
    margin-top: 1px;
    font-size: 14px;
    font-family: "微软雅黑"
}

.inputbox {
    border: 1px solid #000000;
    font: 14px/16px "宋体";
    height: 16px;
    padding: 5px;
    width: 90px;
    vertical-align: middle;
}

.searchform .controlbar {
    text-align: center;
    padding: 14px 0 18px 0;
}

.service_intro {
    border-top: 1px solid #7b7b7b;
    font-family: "微软雅黑";
}

.service_intro .txtlist {
    background: url(img/icon.png) 0 -320px repeat-x;
}

.service_intro .txtlist li {
    float: left;
    width: 175px;
    text-align: center;
    padding: 0 37px 32px 30px;
}

.service_intro h6 {
    font: 18px/1 "微软雅黑";
    color: #ffc300;
    padding: 15px 0 8px 0;
}

.service_intro .txtlist p {
    line-height: 16px;
    color: #eeeeee;
}

.service_intro .linkmore {
    padding-bottom: 23px;
    text-align: center;
}

.service_intro .linkmore a {
    color: #FFFFFF;
}

.recommbox {
    border: 1px solid #0f0f0f;
    width: 460px;
    background: #616161;
    float: left;
}

.slideshow {
    width: 460px;
    height: 220px;
    overflow: hidden;
    position: relative;
    zoom: 1;
}

.slideshow .imgbox ul {
    white-space: nowrap;
    position: relative;
}

.slideshow .imgbox li {
    display: inline-block;
    *display: inline;
    zoom: 1;
}

.slideshow .triggle_bar {
    position: absolute;
    right: 19px;
    bottom: 11px;
    height: 5px;
    font-size: 0;
}

.slideshow .triggle_bar a {
    display: inline-block;
    height: 5px;
    line-height: 5px;
    width: 20px;
    background: #333333;
    margin-left: 5px;
}

.slideshow .triggle_bar a.on, .slideshow .triggle_bar a:hover {
    background: #9b6c01;
}

.recommbox .tips {
    border-top: 1px solid #303030;
    color: #FFFFFF;
    padding-bottom: 17px;
    _padding-bottom: 19px;
}

.recommbox .tips p {
    font: 14px/16px "微软雅黑";
    height: 16px;
    padding-top: 17px;
    text-align: center;
}

.recommbox .tips a {
    font: 16px/1 "微软雅黑";
    color: #e7c127;
    padding: 0 35px 0 10px;
    background: url(img/icon.png) right -450px no-repeat;
}

/************************************part02***********************************/
.part02 {
    margin: 10px 0;
}

.chooseReason, .companyIntro {
    border: 2px solid #999999;
    float: left;
    font-family: "微软雅黑";
    font-size: 14px;
}

.chooseReason {
    width: 511px;
}

.chooseReason h6 {
    font: 28px/1 "微软雅黑";
    padding: 10px 0 0 24px;
}

.chooseReason .txtlist {
    padding: 15px 0 15px 26px;
}

.chooseReason li {
    line-height: 24px;
    height: 24px;
    background: url(img/icon.png) 0 -80px no-repeat;
    padding-left: 23px;
    padding-top: 4px;
}

.companyIntro {
    width: 456px;
    margin-left: 5px;
    display: inline;
}

.companyIntro .intro {
    position: relative;
    zoom: 1;
    line-height: 28px;
    color: #000000;
    text-indent: 8em;
    padding: 27px 14px 10px;
}

.companyIntro img {
    position: absolute;
    left: 14px;
    top: 14px;
}

/************************************part03***********************************/
.part03 {
    margin: 10px 0;
}

.traffic_consult, .bruise_consult, .lawyer_area, .news {
    border: 1px solid #5d5d5d;
    float: left;
    font-family: "微软雅黑";
    font-size: 12px;
}

.traffic_consult {
    width: 253px;
    font-family: "微软雅黑";
    font-size: 12px;
}

.bruise_consult, .lawyer_area, .news {
    width: 253px;
    margin-left: 5px;
    display: inline;
}

.news {
    width: 225px;
}

.mb_hd {
    height: 39px;
    font: bold 14px/39px "微软雅黑";
    background: #828282;
    color: #FFFFFF;
    padding-left: 14px;
}

.part03 .bd {
    overflow: hidden;
    zoom: 1;
}

.part03 .imgbox {
    float: left;
    border: 1px solid #535353;
    width: 90px;
    height: 90px;
    margin: 8px 7px 0 7px;
    display: inline;
}

.part03 .txtlist {
    overflow: hidden;
    zoom: 1;
    height: 145px;
    padding-top: 5px;
}

.part03 .txtlist li {
    line-height: 24px;
    background: url(img/icon.png) 0 -105px no-repeat;
    padding-left: 7px;
}

.part03 .linkmore {
    padding-bottom: 8px;
}

.news .txtlist {
    padding: 5px 15px 0 10px;
}

/************************************part04***********************************/
.part04 {
    margin: 10px 0;
}

.reception_center, .key_knowledge {
    border: 1px solid #000000;
    float: left;
    font-family: "微软雅黑";
    font-size: 14px;
}

.reception_center {
    width: 513px;
}

.reception_center .bd {
    overflow: hidden;
    zoom: 1;
    padding-bottom: 17px;
}

.reception_center .imgbox {
    float: left;
    width: 238px;
    height: 155px;
    border: 1px solid #cacaca;
    margin: 7px 4px 0 7px;
    display: inline;
    _margin-right: 1px;
}

.reception_center .citylist {
    overflow: hidden;
    zoom: 1;
    padding-top: 10px;
}

.reception_center .citylist a {
    float: left;
    word-break: keep-all;
    padding: 0 10px;
    line-height: 28px;
    height: 28px;
    background: url(img/icon.png) 0 -170px no-repeat;
    color: #000000;
    margin-left: -1px;
}

.key_knowledge {
    width: 458px;
    margin-left: 5px;
    display: inline;
    height: 232px;
}

.key_knowledge .txtlist {
    padding: 10px 8px 0 22px;
    font-family: "微软雅黑";
    font-size: 14px;
}

.key_knowledge .txtlist li {
    line-height: 24px;
    background: url(img/icon.png) 0 -105px no-repeat;
    padding-left: 7px;
    padding-top: 4px;
    overflow: hidden;
    zoom: 1;
}

.key_knowledge .txtlist a {
    color: #000000;
    float: left;
}

.key_knowledge .txtlist b {
    float: left;
}

/************************************part05***********************************/
.part05 {
    margin: 10px 0 0 0;
}

.cooperate, .flash_show {
    border: 1px solid #000000;
    float: left;
}

.cooperate {
    width: 513px;
    font-family: "微软雅黑";
    font-size: 14px;
}

.cooperate .imgbox {
    padding: 9px 0 13px 15px;
}

.flash_show {
    width: 458px;
    margin-left: 5px;
    display: inline;
    height: 126px;
    position: relative;
    overflow: hidden;
    font-family: "微软雅黑";
    font-size: 14px;
}
.recommbox .tips {
    border-top: 1px solid #303030;
    color: #FFFFFF;
    padding-bottom: 17px;
    _padding-bottom: 19px;
}

.recommbox .tips .emphasize, .recommbox .tips p {
    font: 14px/25px "微软雅黑";
    height: 25px;
    padding-top: 8px;
    text-align: center;
    overflow: hidden;
}

.recommbox .tips .emphasize {
    width: 380px;
    margin: 0 auto;
}

.recommbox .tips .emphasize span {
    float: left;
    height: 25px;
    overflow: hidden;
}

.recommbox .clickhere {
    float: left;
    position: relative;
    margin-top: -3px;
}

.recommbox .clickhere p {
    padding: 0;
    width: 110px;
    overflow: hidden;
}

.recommbox .linkmore {
    width: 110px;
    position: absolute;
    top: 0;
    left: 0;
}

.recommbox .linkmore a {
    display: block;
    width: 110px;
    height: 25px;
}

.flash_show .linkmore {
    width: 458px;
    height: 126px;
    font-size: 0;
    position: absolute;
    right: 0px;
    bottom: 0;
    padding: 0;
}

.flash_show .linkmore a {
    display: block;
    height: 126px;
    font-size: 0;
    background: url(img/blank.png) no-repeat;
}