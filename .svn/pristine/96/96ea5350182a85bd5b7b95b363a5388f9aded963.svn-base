#Nav ul li ul {
    display: none;
    position: absolute;
    border-bottom: none;
    margin: 0;
}

#Nav ul li:hover ul {
    display: block;
    position:relative;
    z-index: 9999;
}
#Nav ul li a {
    color: #000;
    text-decoration: none;
    display: block;
}

#Nav ul li a:hover {
    color: #F00;
}

#Nav ul li {
    list-style: none;
    margin: 0,auto;
}
#menu ul li a {
    color: #000;
    text-decoration: none;
    display: block;
}

#menu ul li a:hover {
    color: #F00;
    background:#003795;
}

#menu ul li {
    list-style: none;
    margin: 0,auto;
    padding: 0px 8px;
    background: #eee;
    line-height: 26px;
    border-bottom: 1px solid #CCC;
    width: 90px;
    position: relative;
    float: left;
}

#menu ul li ul {
    display: none;
    position: absolute;
    border: 1px solid #ccc;
    border-bottom: none;
    margin: 0;
}

#menu ul li:hover ul {
    display: block;
    position:relative;
    z-index: 9999;
}
