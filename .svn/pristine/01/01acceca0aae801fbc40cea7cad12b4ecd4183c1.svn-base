/********** global **********/
* {
    font-size: 14px;
    font-family: "微软雅黑";
}

body {
    margin: 0;
    padding: 0;
    background: white;
}

img {
    border: 0;
}

a {
    text-decoration: none;
}

a:link, a:visited, a:active {
    text-decoration: none;
}

a:hover {
    /*text-decoration:underline;*/
}

/********** header **********/
.header {
    width: 100%;
    height: 30px;
    background: url(header_bg.jpg) #ffffff repeat-x 0 0;
    text-align: center;
    background-color: #FBFDE7;
    font-family: "微软雅黑";
}

.header .padder {
    width: 972px;
    margin: 0 auto;
    background: url(header_bg.jpg) #ffffff repeat-x 0 0;
    padding-bottom: 4px;
    text-align: left;
}

.header .padder .menunav {
    background: url(../images/vertical.jpg) repeat-x 0 -36px;
    height: 36px;
}

.header .padder .navLeftBg {
    background: url(../images/vertical.jpg) no-repeat 0 0;
    height: 36px;
}

.header .padder .navRightBg {
    /*background: url(../images/icons.jpg) no-repeat right -143px;*/
    height: 36px;
}

.header .padder .menunav .mainNav {
    padding-left: 24px;
    position: absolute;
}

.header .padder .menunav .mainNav a:link, .header .padder .menunav .mainNav a:visited, .header .padder .menunav .mainNav a:active {
    width: 90px;
    height: 36px;
    display: inline-block;
    text-align: center;
    color: #ffffff;
    font-weight: bold;
    font-size: 13px;
    line-height: 36px;
    margin-left: 11px;
}

.header .padder .menunav .mainNav a:hover {
    text-decoration: none;
}

.header .padder .menunav .mainNav a.actived:link, .header .padder .menunav .mainNav a.actived:visited, .header .padder .menunav .mainNav a.actived:hover, .header .padder .menunav .mainNav a.actived:active {
    background: url(../images/icons.jpg) no-repeat 0 -27px;
    color: #473505;
}

.header .padder .secondNav {
    line-height: 21px;
    text-align: left;
    background-color: #FBFDE7;
    height: 30px;
}

.header .padder .secondNav a {
    color: #473505;
    display: inline-block;
    padding: 0 8px;
    margin-right: 19px;
}

.header .padder .secondNav a.actived {
    font-weight: bold;
}

.header .padder .secondNav .subNav1 {
    display: none;
}

.header .padder .secondNav .subNav2 {
    padding-left: 150px;
    display: none;
}

.header .padder .secondNav .subNav3 {
    padding-left: 250px;
    display: none;
}

.header .padder .secondNav .subNav4 {
    padding-left: 350px;
    display: none;
}

.header .padder .secondNav .subNav5 {
    padding-left: 450px;
    display: none;
}

.header .padder .secondNav .subNav6 {
    padding-left: 550px;
    display: none;
}

.header .padder .secondNav .subNav7 {
    padding-left: 650px;
    display: none;
}